<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Toronto.Concerts.MAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Toronto.Concerts.MAUI"
    xmlns:pages="clr-namespace:Toronto.Concerts.MAUI.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="Concerts 416"
    NavigationPage.HasBackButton="False"
    NavigationPage.HasNavigationBar="False"
    >
    <FlyoutItem FlyoutDisplayOptions="AsSingleItem">
        <ShellContent ContentTemplate="{DataTemplate pages:StartupPage}"
                          Route="startuppage" 
                          Shell.BackgroundColor="AntiqueWhite"
                          Shell.TabBarIsVisible="False"
            >
        </ShellContent>
    </FlyoutItem>
</Shell>
